<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundSquareProgressView">
        <attr name="radius" format="dimension" />
        <attr name="bg_color" format="color" />
        <attr name="font_name" format="string" />
        <attr name="text" format="string" />
        <attr name="text_color" format="color" />
        <attr name="text_gravity">
            <flag name="left" value="3"/>
            <flag name="center" value="17"/>
            <flag name="right" value="5"/>
        </attr>
        <attr name="progress_percent" format="integer" />
        <attr name="progress_width" format="dimension" />
        <attr name="progress_color" format="color" />
        <attr name="progress_bg_color_static" format="color" />
        <attr name="progress_bg_color_dynamic" format="color" />
        <attr name="progress_start_angle" format="integer" />
    </declare-styleable>
</resources>